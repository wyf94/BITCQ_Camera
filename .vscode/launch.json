    "version": "0.2.0",
    "configurations": [
        {
//          "preLaunchTask": "catkin_make: build",
            "name": "ROS: Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/nvidia/ros_ws/devel/lib/bitcq_camera/bitcq_camera_node",
            "args": ["device_type=dahua_sdk device_address=/dev/video0 image_width=1920 image_height=1080 fps=30 pixel_format=uyvy"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}

